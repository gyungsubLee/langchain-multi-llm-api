# Copy to .env and fill real credentials or set MOCK=true to use mock responses
# For OpenAI (ChatOpenAI via langchain-openai)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# For Google GenAI (langchain-google-genai). Set credentials according to Google SDK or env.
GOOGLE_API_KEY=
GOOGLE_MODEL=gemini-2.5-flash

# For Anthropic
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# If MOCK=true server returns canned responses (default)
MOCK=true
